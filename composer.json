        "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
        "laravel/framework": "^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "SteadfastCollective\\LaravelDailyco\\": "src"
        }
    }
}
